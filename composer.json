{
  "name": "teurons/neptune-php",
  "description": "PHP Helpers for Neptune",
  "keywords": [
    "Notifications"
  ],
  "homepage": "https://github.com/teurons/neptune-php",
  "license": "MIT",
  "authors": [
    {
      "name": "Rajiv Seelam",
      "email": "rajiv@teurons.com",
      "homepage": "https://teurons.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php-curl-class/php-curl-class": "^9.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4",
    "orchestra/testbench": "~3.7.0"
  },
  "autoload": {
    "psr-4": {
      "Teurons\\Neptune\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Teurons\\Neptune\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Teurons\\Neptune\\NeptuneServiceProvider"
      ]
    }
  }
}
